{
  "manifest_version": 2,

  "name": "Study Buddy Kitty",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",
  "background": {"scripts":["js/background.js"]},
  "permissions": [
	"webRequest",
	"webRequestBlocking",
	"*://*.yahoo.com/*"
  ],
  "browser_action": {
    "default_icon": "images/icon_on.png",
    "default_popup": "popup.html"
  }
}