{
  "manifest_version": 2,

  "name": "Study Kitten",
  "description": "Producitivity app with a kitten!",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts":[{
    "js": ["jquery-1.11.1.min.js", "background.js", "popup.js"],
    "matches": ["http://*/*", "https://*/*"]
  }
  ],
  "permissions": [
    "https://secure.flickr.com/"
  ]
}
