{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",
  "background": {"scripts":["background.js"]},
  "permissions": [
	"webRequest",
	"webRequestBlocking",
	"*://*.yahoo.com/*"
  ],
  "browser_action": {
    "default_icon": "icon_on.png",
    "default_popup": "popup.html"
  }
}