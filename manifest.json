{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",
  "background": {"scripts":["background.js"]},
  "permissions": [
    "https://secure.flickr.com/",
	"webRequest",
	"webRequestBlocking",
	"*://*.yahoo.com/",
	"*://*.google.com/",
	"*://*.evil.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}